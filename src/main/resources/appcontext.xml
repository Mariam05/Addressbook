<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="model" class="AddressBook.AddressBook">
    </bean>
    <bean id="controller" class="AddressBook.AddressBookController">
        <constructor-arg ref="model" />
    </bean>
    <bean id="mainframe" class="AddressBook.MainFrame">
        <constructor-arg ref="controller" />
        <property name="title" value="My Address Book" />
    </bean>
</beans>